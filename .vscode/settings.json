{
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "src/**/*.js": false,
        "src/**/*.js.map": true,
        "test/**/*.js": true,
        "test/**/*.js.map": true,
        "node_modules": true,
        "start.js": true,
        "start.js.map": true,
        "config.js": true,
        "config.js.map": true
    }
}